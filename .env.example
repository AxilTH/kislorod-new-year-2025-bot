# Copy this file to .env and fill values

# Telegram bot token
# TOKEN=your_telegram_token_here

# Admin token for FastAPI admin endpoints
# ADMIN_TOKEN=some_secure_admin_token

# App register codes
# REGISTER_CODE=register_code_here

# Postgres settings for Docker (used by docker-compose.yml)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=kislorod
POSTGRES_PORT=5432
POSTGRES_HOST=127.0.0.1

# Optionally set full SQLAlchemy URL. If not set, config.py will build it from POSTGRES_* variables
# TARGET_DB_URL=postgresql+asyncpg://postgres:postgres@127.0.0.1:5432/kislorod

# DEFAULT_DB_URL is used by create-database step if needed (can be same DB server)
# DEFAULT_DB_URL=postgresql+asyncpg://postgres:postgres@127.0.0.1:5432/postgres
